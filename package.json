{
  "name": "meeting-room",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --run",
    "test:unit:dev": "vitest --ui",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test": "run-s test:unit",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . -D correctness --ignore-path .gitignore",
    "lint:oxlint:fix": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:fix": "run-s lint:eslint:fix lint:oxlint:fix",
    "lint": "run-s lint:oxlint lint:eslint lint:prettier",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "dexie": "^4.0.11",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.18",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-istanbul": "3.0.9",
    "@vitest/eslint-plugin": "^1.1.38",
    "@vitest/ui": "3.0.9",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-oxlint": "^0.15.15",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "~10.0.0",
    "fake-indexeddb": "^6.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.15",
    "prettier": "3.5.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass-embedded": "^1.86.0",
    "start-server-and-test": "^2.0.11",
    "typescript": "~5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vuetify": "^2.1.0",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "yarn@4.7.0"
}
